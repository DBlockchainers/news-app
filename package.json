{
    "name": "adminlte-3-react",
    "version": "0.1.0",
    "private": true,
    "scripts": {
        "start": "craco start",
        "start2": "craco --openssl-legacy-provider start",
        "start3": "DISABLE_ESLINT_PLUGIN=true craco --openssl-legacy-provider start",
        "build": "craco build",
        "dev": "craco test",
        "eject": "react-scripts eject",
        "format": "prettier-eslint $PWD/'src/**/*.{js,json}' --write",
        "lint": "eslint src/**/*.js"
    },
    "eslintConfig": {
        "extends": "react-app"
    },
    "browserslist": {
        "production": [
            ">0.2%",
            "not dead",
            "not op_mini all"
        ],
        "development": [
            "last 1 chrome version",
            "last 1 firefox version",
            "last 1 safari version"
        ]
    },
    "browser": {
        "fs": false,
        "path": false,
        "os": false
    },
    "dependencies": {
        "@coreui/coreui": "^4.0.0",
        "@coreui/icons": "^2.1.0",
        "@coreui/icons-react": "^2.0.1",
        "@coreui/react": "^4.1.1",
        "@coreui/react-chartjs": "^2.0.0",
        "@coreui/utils": "^1.3.1",
        "@craco/craco": "^6.4.0",
        "@devexpress/dx-react-chart": "^2.7.6",
        "@devexpress/dx-react-chart-material-ui": "^2.7.6",
        "@fontawesome/fontawesome-svg-core": "1.2.36",
        "@fortawesome/free-brands-svg-icons": "5.15.4",
        "@fontawesome/free-regular-svg-icons": "5.15.4",
        "@fontawesome/free-solid-svg-icons": "5.15.4",
        "@fontawesome/react-fontawesome": "0.1.16",
        "@material-ui/core": "^4.12.3",
        "@reduxjs/toolkit": "1.6.2",
        "@testing-library/jest-dom": "5.15.0",
        "@testing-library/react": "12.1.2",
        "@testing-library/user-event": "13.5.0",
        "axios": "^0.24.0",
        "bootstrap": "^3.4.1",
        "chart.js": "^1.1.1",
        "craco-alias": "3.0.1",
        "ethers": "^5.5.3",
        "formik": "2.2.9",
        "gatekeeper-client-sdk": "0.4.2",
        "i18next": "21.4.2",
        "i18next-browser-languagedetector": "6.1.2",
        "js-cookie": "^3.0.1",
        "luxon": "2.1.1",
        "moment": "^2.29.1",
        "moment-timezone": "^0.5.34",
        "node-sass": "6.0.1",
        "react": "17.0.2",
        "react-bootstrap": "^1.6.4",
        "react-chartjs": "^1.2.0",
        "react-dom": "17.0.2",
        "react-hot-toast": "^2.2.0",
        "react-i18next": "11.14.2",
        "react-moment": "^1.1.1",
        "react-redux": "7.2.6",
        "react-router-dom": "^5.3.0",
        "react-scripts": "4.0.3",
        "react-toastify": "^8.1.0",
        "redux": "4.1.2",
        "sweetalert2": "^6.6.1",
        "use-history": "^1.4.1",
        "uuid": "8.3.2",
        "yup": "0.32.11"
    },
    "devDependencies": {
        
        "husky": "7.0.4",
        "lint-staged": "12.0.1",
        "prettier": "2.4.1",
        "prettier-eslint": "13.0.0",
        "prettier-eslint-cli": "5.0.1"
    },
    "lint-staged": {
        "**/*.js": [
            "eslint --fix",
            "prettier-eslint --write"
        ]
    },
    "husky": {
        "hooks": {
            "pre-commit": "lint-staged"
        }
    }
